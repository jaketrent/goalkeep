{
  "name": "goalkeep",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-test-utils": "^0.3.2",
    "autoprefixer": "7.1.1",
    "aws-sdk": "^2.82.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.1.1",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "body-parser": "^1.17.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "cors": "^2.8.3",
    "css-loader": "0.28.4",
    "dotenv": "^4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "graphql": "^0.10.3",
    "graphql-server-express": "^1.0.0",
    "graphql-tools": "^1.1.0",
    "html-webpack-plugin": "2.29.0",
    "is-valid-email": "0.0.2",
    "jest": "20.0.4",
    "knex": "^0.13.0",
    "object-assign": "4.1.1",
    "pg": "^7.0.2",
    "postcss-cssnext": "^3.0.0",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "qs": "^6.5.0",
    "react": "^16.0.0-beta.3",
    "react-apollo": "^1.4.3",
    "react-cookie": "^2.0.8",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^16.0.0-beta.3",
    "react-error-overlay": "^1.0.9",
    "react-gravatar": "^2.6.3",
    "react-router-dom": "^4.1.2",
    "react-styleable": "^3.0.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "uuid": "^3.1.0",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-preset-flow": "^6.23.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.1",
    "flow-bin": "^0.51.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.2"
  },
  "scripts": {
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "precommit": "lint-staged",
    "flow": "flow",
    "start": "concurrently \"node scripts/start.js\" \"nodemon src/server\"",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "deploy:client": "cross-env NODE_ENV=production npm run build && aws s3 sync build/ s3://goalkeep",
    "deploy:server": "git push heroku master",
    "heroku-postbuild": "npm run db:migrate"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/prettier --single-quote --no-semi --write",
      "git add"
    ],
    "*.css": [
      "node_modules/.bin/prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      "transform-inline-environment-variables"
    ],
    "presets": [
      "flow",
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
